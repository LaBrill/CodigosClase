funcion = 0
while (True):
    utime.sleep(0.2)
     
    def secuencia1():
        
        print ("Rojo presionado")
        
        def derecha():
            for i in ledt:
              for j in range (1):
                  i.value(not i.value())
                  utime.sleep(0.15)
        def izquierda():
            for i in reversed(ledt):
              for j in range (1):
                  i.value(not i.value())
                  utime.sleep(0.15)
        for funcion in range (2):
            derecha()
            izquierda()
            izquierda()
            derecha()
        
    def secuencia2():
        print ("Amarillo presionado")
        def derecha():
            for elemento in todos:
                elemento.value(1)
                utime.sleep_ms(50)
                elemento.value(0)
                utime.sleep(0.05)
        
        def izquierda():
            for elemento in reversed(todos):
                elemento.value(1)
                utime.sleep(0.05)
                elemento.value(0)
                utime.sleep(0.05)
                
        for funcion in range(2):
            derecha()
            izquierda()
        
    def secuencia3():
        
