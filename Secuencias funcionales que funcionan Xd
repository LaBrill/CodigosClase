    def secuencia13():
        print("rojo azul y verde ")
        def derecha():
            for i in mitad:
              for j in range (1):
                  i.value(not i.value())
                  utime.sleep(0.5)
                  
        def izquierda():
            for i in mitad2:
              for j in range (1):
                  i.value(not i.value())
                  utime.sleep(0.5)
        for funcion in range (1):
            derecha()
        for function in range(1):
            izquierda()
    
