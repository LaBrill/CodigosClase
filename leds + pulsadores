def secuencia1():
        
        print ("Rojo presionado")
        
        def derecha():
            for i in ledt:
              for j in range (1):
                  i.value(not i.value())
                  sleep_ms(velocidad)
        def izquierda():
            for i in reversed(ledt):
              for j in range (1):
                  i.value(not i.value())
                  sleep_ms(velocidad)
        for funcion in range (2):
            derecha()
            izquierda()
            izquierda()
            derecha()
        
def secuencia2():
        print ("Amarillo presionado")
        def derecha():
            for elemento in todos:
                elemento.value(1)
                sleep_ms(velocidad)
                elemento.value(0)
                sleep_ms(velocidad)
        
        def izquierda():
            for elemento in reversed(todos):
                elemento.value(1)
                sleep_ms(velocidad)
                elemento.value(0)
                sleep_ms(velocidad)
                
        for funcion in range(2):
            derecha()
            izquierda()
        
def secuencia3():
        
        print ("Azul presionado")
        
        lede.value(1) , ledf.value(1)
        sleep_ms(velocidad)
        lede.value(0) , ledf.value(0)

        ledd.value(1) , ledg.value(1)
        sleep_ms(velocidad)
        ledd.value(0) , ledg.value(0)
        

        ledc.value(1) , ledh.value(1)
        sleep_ms(velocidad)
        ledc.value(0) , ledh.value(0)
        
        ledb.value(1) , ledi.value(1)
        sleep_ms(velocidad)
        ledb.value(0) , ledi.value(0)

        leda.value(1) , ledj.value(1)
        sleep_ms(velocidad)
        leda.value(0) , ledj.value(0)

        ledb.value(1) , ledi.value(1)
        sleep_ms(velocidad)
        ledb.value(0) , ledi.value(0)

